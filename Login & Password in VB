Imports System

Module Program
    Sub Main(args As String())
        Console.WriteLine("Enter Your UserName : ") 'Display Request To user
        Dim Username As String                                                 ' Declaring a Variable
        Username = Console.ReadLine()                               ' Get input from user and asseign it to the variable 

        Console.WriteLine("Enter Your Password : ")
        Dim Password As String
        Password = Console.ReadLine()

        If Username = "Toussia" Or Username = "Toufik" And Password = "1233" Then         'Condition if Usernam and password are correct 
            Console.WriteLine("Access Granted ")            'Display This Message 
        Else                                                                                                                                                                                                  'Condition if Usernam and password are "not" correct 
            Console.WriteLine("Access Denied ")             '  'Display This Message 

        End If
        End Sub
