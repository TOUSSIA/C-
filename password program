using System;


namespace If_Else
{

    class Program
    {
        static void Main(string[] args)
        {
            String Pass;
            Console.Write("Enter Password : ");
            Pass = Console.ReadLine();
            if (Pass == "1987")
            {
                Console.WriteLine("Access Granted");
                Console.ReadKey();
            }
            else
            {
                Console.WriteLine("Access Denied");
                Console.ReadKey();
            }
        }
    }
}
