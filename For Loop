using System;
namespace increment
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 0; i < 5; i = i + 1) ///for value = 0 , repeat until <5, value = value+1
            {
        Console.WriteLine(i+1);
       }     
        Console.ReadKey();
            }
        }
 }
